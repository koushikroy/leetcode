    "nums = [0,1,0,3,12]\n",
    "count_zeros = 0\n",
    "count_non_zeros = 0\n",
    "for i in range(len(nums)):\n",
    "    if nums[i] == 0:\n",
    "        count_zeros += 1\n",
    "    else:\n",
    "        nums[count_non_zeros] = nums[i]\n",
    "        count_non_zeros += 1\n",
    "nums[count_non_zeros:] = [0]*count_zeros\n",
    "nums"
   ]
